<?xml version="1.0" encoding="utf-8"?>
<odoo>    
        <record id="view_partner_iibb_sircar_misiones_form" model="ir.ui.view">
            <field name="name">res.partner.form</field>
            <field name="model">res.partner</field>
            <!-- Use the partner property form defined in l10n_ar_partner which provides the 'fiscal_data' page -->
            <field name="inherit_id" ref="l10n_ar_partner.view_partner_property_form"/>
            <field name="arch" type="xml">

                <!-- Insert a new page after the existing 'fiscal_data' page using xpath -->
                <xpath expr="//page[@name='fiscal_data']" position="after">
                    <page name="fiscal_data_sircar" string="Ret./Perc. IIBB SIRCAR Misiones">
                        <group>
                            <group>
                                <field name="alicuota_ret_misiones_3"/>
                                <field name="alicuota_ret_misiones_1"/>
                                <field name="alicuota_ret_misiones"/>
                            </group>
                            <group>
                                <field name="alicuota_per_misiones_3"/>
                                <field name="alicuota_per_misiones_1"/>
                                <field name="alicuota_per_misiones"/>
                            </group>
                        </group>
                    </page>
                </xpath>

            </field>
    </record>

</odoo>
